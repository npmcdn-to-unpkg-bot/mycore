<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.mycore.backend.hibernate.tables">
  <class name="MCRDNBTRANSFERRESULTS" table="MCRDNBTRANSFERRESULTS">

    <id name="id" column="ID">
      <generator class="identity" />
    </id>

    <property name="protocolType" type="string" column="PROTOCOLTYPE" length="9" not-null="true" />

    <property name="tpName" type="string" column="TP_NAME" length="9" not-null="true" />

    <property name="deliveryRole" type="string" column="DELIVERYROLE" length="32" not-null="true" />

    <property name="objectId" type="string" column="OBJECTID" length="124" not-null="false" />

    <property name="transferPackageArchived" type="boolean" column="TRANSFERPACKAGEARCHIVED" not-null="false" />

    <property name="errorMessage" type="string" column="ERRORMESSAGE" length="1024" not-null="false" />

    <property name="errorModule" type="string" column="ERRORMODULE" length="64" not-null="false" />

    <property name="date" type="timestamp" column="DATE" not-null="true" />

  </class>
</hibernate-mapping>