<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MyCoReWebPage>

<MyCoReWebPage>

<section title="Bearbeiten eines Derivates" xml:lang="de">
</section>

<section title="Edit an object (derivate)" xml:lang="en">
</section>

<section title="معالجة عنصر البيانات" xml:lang="ar">
</section>

<section title="Mengedit sebuah obyek (derivat)" xml:lang="id">
</section>

<section xml:lang="de" alt="en,id">
  <editor id="editor-commit-derivate">
     <source uri="{sourceUri}" />
     <target type="servlet" name="MCRCheckCommitDerivateServlet" method="post" format="xml"/>
     <cancel url="{cancelUrl}" />

     <components root="editor-derivate" var="/mycorederivate">
      <include uri="webapp:editor/imports-common.xml"/>
      <include uri="webapp:editor/imports-editor-derivate-common.xml"/>
      <include uri="webapp:editor/imports-editor-derivate-ltr.xml"/>
     </components>
  </editor>
</section>

<section xml:lang="ar">
<div align="right">
  <editor id="editor-commit-derivate-ar">
     <source uri="{sourceUri}" />
     <target type="servlet" name="MCRCheckCommitDerivateServlet" method="post" format="xml"/>
     <cancel url="{cancelUrl}" />

     <components root="editor-derivate" var="/mycorederivate">
      <include uri="webapp:editor/imports-common.xml"/>
      <include uri="webapp:editor/imports-editor-derivate-common.xml"/>
      <include uri="webapp:editor/imports-editor-derivate-rtl.xml"/>
     </components>
  </editor>
</div>
</section>
	
</MyCoReWebPage>


