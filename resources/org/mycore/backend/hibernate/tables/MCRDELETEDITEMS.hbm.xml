<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.mycore.backend.hibernate.tables">
  <class name="MCRDELETEDITEMS" table="MCRDELETEDITEMS">
    <composite-id class="MCRDELETEDITEMSPK" name="key">
      <key-property name="identifier" type="string" column="IDENTIFIER" length="128" />
      <key-property name="dateDeleted" type="timestamp" column="DATE_DELETED" />
    </composite-id>
    <property name="userid"/>
    <property name="ip"/>
  </class>
</hibernate-mapping>