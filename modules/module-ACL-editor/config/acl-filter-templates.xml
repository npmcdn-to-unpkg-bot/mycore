<?xml version="1.0" encoding="ISO_8859-1"?>
<module-acl-editor>
    <acl-filters>
        <!-- element <acl-filter> can have attribute value of type either @type="permission" or @type="objectID" -->
        <acl-filter type="permission">
            <value>read</value>
            <label xml:lang="de">Lesen</label>
            <label xml:lang="en">Read</label>
        </acl-filter>
        <acl-filter type="permission">
            <value>write</value>
            <label xml:lang="de">Editieren</label>
        </acl-filter>
        <acl-filter type="permission">
            <value>writewf</value>
            <label xml:lang="de">Objekte im Workflow editieren</label>
        </acl-filter>
        <acl-filter type="permission">
            <value>writedb</value>
            <label xml:lang="de">Objekte editieren</label>
        </acl-filter>
        <acl-filter type="permission">
            <value>deletewf</value>
            <label xml:lang="de">Objekte im Workflow löschen</label>
        </acl-filter>
        <acl-filter type="permission">
            <value>deletedb</value>
            <label xml:lang="de">Objekte löschen</label>
        </acl-filter>
        <acl-filter type="permission">
            <value>deletedb</value>
            <label xml:lang="de">Objekte löschen</label>
        </acl-filter>
        <acl-filter type="objectID">
            <value>default</value>
            <label xml:lang="de">Standard</label>
        </acl-filter>
        <acl-filter type="objectID">
            <value>Docportal_document_*</value>
            <label xml:lang="de">Dokument(e)</label>
        </acl-filter>
        <acl-filter type="objectID">
            <value>Docportal_author_*</value>
            <label xml:lang="de">Autor(en)</label>
        </acl-filter>
        <acl-filter type="objectID">
            <value>webpage*</value>
            <label xml:lang="de">Statische Webseite(n)</label>
        </acl-filter>
    </acl-filters>
</module-acl-editor>