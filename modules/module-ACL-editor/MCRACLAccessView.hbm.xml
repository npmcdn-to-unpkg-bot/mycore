<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.mycore.services.acl.MCRACLAccessView" table="MCRACCESS">
    <composite-id class="org.mycore.backend.hibernate.tables.MCRACCESSPK" name="key">
      <key-property name="acpool" type="string" column="ACPOOL" length="64" />
      <key-property name="objid" type="string" column="OBJID" length="64" />
    </composite-id>
    <property name="creator" type="string" column="CREATOR" length="64" not-null="true" unique="false" />
    <property name="creationdate" type="timestamp" column="CREATIONDATE" length="64" not-null="true" unique="false" />
    <many-to-one name="rule" class="org.mycore.backend.hibernate.tables.MCRACCESSRULE" column="RID" update="false" insert="false"/>
  </class>
</hibernate-mapping>

