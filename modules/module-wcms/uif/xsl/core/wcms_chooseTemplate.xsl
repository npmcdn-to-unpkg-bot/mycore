<?xml version="1.0" encoding="ISO-8859-1"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<!-- ===================================================================================== -->
	<!-- add here the xsl stylesheet which is your template by: -->
	<!-- adding "	<xsl:include href="template_MYTEMPLATE.xsl" />" -->
	<!-- ===================================================================================== -->
	<xsl:include href="template_docportal.xsl" />
	<xsl:include href="template_wcms.xsl" />
	<xsl:include href="template_mycore-de.xsl" />
	<xsl:include href="template_mycore-de-2.xsl" />
	<!-- ===================================================================================== -->
	<!-- ===================================================================================== -->
	<!-- ........................................................................................................................................................................................................................ -->
	<xsl:template name="chooseTemplate">
		<xsl:param name="PrivOfUser" />
		<xsl:param name="template" />
		<xsl:param name="browserAddress" />
		<xsl:choose>
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- template_docportal_index -->
			<!-- ===================================================================================== -->
			<xsl:when test=" $template = 'template_docportal' " >
				<xsl:call-template name="template_docportal">
					<xsl:with-param name="template" select="$template" />
					<xsl:with-param name="browserAddress" select="$browserAddress" />
				</xsl:call-template>
			</xsl:when>
			<!-- ===================================================================================== -->
			<!-- END: template_docportal_index -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- template_wcms_index -->
			<!-- ===================================================================================== -->
			<xsl:when test=" $template = 'template_wcms' " >
				<xsl:call-template name="template_wcms">
					<xsl:with-param name="template" select="$template" />
					<xsl:with-param name="browserAddress" select="$browserAddress" />
				</xsl:call-template>
			</xsl:when>
			<!-- ===================================================================================== -->
			<!-- END: template_wcms_index -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- template_mycoresample_index -->
			<!-- ===================================================================================== -->
			<xsl:when test=" $template = 'template_mycore-de' " >
				<xsl:call-template name="template_mycore-de">
					<xsl:with-param name="template" select="$template" />
					<xsl:with-param name="browserAddress" select="$browserAddress" />
				</xsl:call-template>
			</xsl:when>
			<!-- ===================================================================================== -->
			<!-- END: template_mycoresample_index -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- template_mycoresampleAlternative_index -->
			<!-- ===================================================================================== -->
			<xsl:when test=" $template = 'template_mycore-de-2' " >
				<xsl:call-template name="template_mycore-de-2">
					<xsl:with-param name="template" select="$template" />
					<xsl:with-param name="browserAddress" select="$browserAddress" />
				</xsl:call-template>
			</xsl:when>
			<!-- ===================================================================================== -->
			<!-- END: template_mycoresampleAlternative_index -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
			<!-- set name of default template -->
			<!-- ===================================================================================== -->
			<xsl:otherwise>
				<xsl:call-template name="template_docportal">
					<xsl:with-param name="template" 
						select="document($navigationBase) /navigation/@template" 
						/>
					<xsl:with-param name="browserAddress" select="$browserAddress" />
				</xsl:call-template>
			</xsl:otherwise>
			<!-- ===================================================================================== -->
			<!-- END: set name of default template -->
			<!-- ===================================================================================== -->
			<!-- ===================================================================================== -->
		</xsl:choose>
	</xsl:template>
	<!-- ================================================================================= -->
</xsl:stylesheet>
