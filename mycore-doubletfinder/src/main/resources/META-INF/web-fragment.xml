<!-- **************************************************************** -->
<!-- Servlets and mappings of doubletfinder                           -->
<!-- **************************************************************** -->

  <servlet id="MCRRedundancyMapServlet">
    <servlet-name>MCRRedundancyMapServlet</servlet-name>
    <servlet-class>org.mycore.frontend.redundancy.servlets.MCRRedundancyMapServlet</servlet-class>
