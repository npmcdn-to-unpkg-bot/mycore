<imports xmlns:xhtml="http://www.w3.org/1999/xhtml">
  <include uri="webapp:editor/editor-mods-external.xml" />

  <!-- TITLE -->
  <text id="mods.title.label">
    <label xml:lang="de">Titel (*)</label>
    <label xml:lang="en">Title (*)</label>
  </text>

  <text id="mods.related.title.label">
    <label xml:lang="de">Titel des Buches (*)</label>
    <label xml:lang="en">Booktitle (*)</label>
  </text>

  <panel id="mods.title">
    <cell anchor="NORTHWEST" var="mods:title">
      <textarea id="mods.title.text" width="60" height="3">
        <condition id="mods.title.cond" required="true">
          <label xml:lang="de">Tragen Sie hier den Titel des Dokumentes ein.</label>
          <label xml:lang="en">Enter the title of the document here.</label>
        </condition>
      </textarea>
    </cell>
  </panel>

  <helpPopup id="mods.title.help" width="400px" height="250px">
    <label xml:lang="de">
      Der Titel des Dokumentes oder Objekts, unter dem es formell bekannt ist.
    </label>
    <label xml:lang="en">
      Title of document by which the resource is formally known.
    </label>
  </helpPopup>
  <!-- TITLE END -->

  <!-- TITLE Translated -->
  <text id="mods.title.translated.label">
    <label xml:lang="de">Titel übersetzt</label>
    <label xml:lang="en">Title translated</label>
  </text>

  <panel id="mods.title.translated">
    <cell anchor="NORTHWEST" var="mods:titleInfo[@type='translated']/mods:title">
      <repeater min="1" max="5" pos="right">
        <panel>
          <cell anchor="NORTHWEST" ref="TFarea1028" />
          <cell row="1" col="2" anchor="NORTHWEST" var="@lang">
            <list type="dropdown" default="de">
              <item value="de">
                <label xml:lang="de">deutsch</label>
                <label xml:lang="en">german</label>
              </item>
              <item value="en">
                <label xml:lang="de">englisch</label>
                <label xml:lang="en">english</label>
              </item>
              <item value="other">
                <label xml:lang="de">sontige</label>
                <label xml:lang="en">other</label>
              </item>
            </list>
          </cell>
        </panel>
      </repeater>
    </cell>
  </panel>

  <helpPopup id="mods.title.translated.help" width="400px" height="250px">
    <label xml:lang="de">
      Der Titel übersetzt. Mit dem "+" button können weitere Übersetzungen des
      Titels hinzugefügt werden.
    </label>
    <label xml:lang="en">
      title translated.
    </label>
  </helpPopup>
  <!-- TITLE Translated END -->

  <!-- LANGUAGE -->
  <text id="mods.languageTerm.label">
    <label xml:lang="de">Sprache (*)</label>
    <label xml:lang="en">Language (*)</label>
  </text>

  <panel id="mods.languageTerm">
    <cell anchor="NORTHWEST" var="mods:languageTerm">
      <panel>
        <hidden var="@authority" default="iso639-3" />
        <hidden var="@type" default="code" />
        <cell col="1">
          <list id="mods.languageTerm.list" type="dropdown" default="ger">
            <item value="ger">
              <label xml:lang="de">deutsch</label>
              <label xml:lang="en">german</label>
            </item>
            <item value="eng">
              <label xml:lang="de">englisch</label>
              <label xml:lang="en">english</label>
            </item>
            <item value="fre">
              <label xml:lang="de">französisch</label>
              <label xml:lang="en">french</label>
            </item>
            <item value="spa">
              <label xml:lang="de">spanisch</label>
              <label xml:lang="en">spanish</label>
            </item>
            <item value="hun">
              <label xml:lang="de">ungarisch</label>
              <label xml:lang="en">hungarian</label>
            </item>
            <item value="pol">
              <label xml:lang="de">polnisch</label>
              <label xml:lang="en">polish</label>
            </item>
            <item value="cze">
              <label xml:lang="de">tschechisch</label>
              <label xml:lang="en">czech</label>
            </item>
            <item value="ita">
              <label xml:lang="de">italienisch</label>
              <label xml:lang="en">italian</label>
            </item>
            <item value="dut">
              <label xml:lang="de">holländisch</label>
              <label xml:lang="en">dutch</label>
            </item>
            <item value="rum">
              <label xml:lang="de">rumänisch</label>
              <label xml:lang="en">romanian</label>
            </item>
            <item value="rus">
              <label xml:lang="de">russisch</label>
              <label xml:lang="en">russian</label>
            </item>
            <item value="por">
              <label xml:lang="de">portugiesisch</label>
              <label xml:lang="en">portuguese</label>
            </item>
            <item value="mis">
              <label xml:lang="de">sonstige</label>
              <label xml:lang="en">other</label>
            </item>
            <condition id="mods.languageTerm.cond" required="true">
              <label xml:lang="de">Bitte wählen Sie einen Wert aus!</label>
              <label xml:lang="en">Please select one entry!</label>
            </condition>
          </list>
        </cell>
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.languageTerm.help" width="400px" height="250px">
    <label xml:lang="de">
      Die Sprache, in welcher das Dokument verfasst ist.
    </label>
    <label xml:lang="en">
      language of document text
    </label>
  </helpPopup>
  <!-- LANGUAGE END -->

  <!-- pub_autor_dok_txt [text] and reviewer -->
  <text id="mods.author.label">
    <label xml:lang="de">Autor(en) (*)</label>
    <label xml:lang="en">Author(s) (*)</label>
  </text>
  
  <panel id="mods.name">
    <hidden var="nameOrPND/@editor.output" />
    <cell col="1" row="1" anchor="NORTHEAST">
      <text>
        <label xml:lang="de">Name oder PND</label>
        <label xml:lang="en">Name or PND</label>
      </text>
    </cell>
    <cell col="2" row="1" anchor="NORTHEAST" var="nameOrPND">
      <textfield id="mods.author.firstname.text" width="31" maxlength="128">
        <condition id="mods.author.firstname.cond" required="true">
          <label xml:lang="de">Bitte geben Sie den Namen der Person (&lt;Name, Vorname&gt;) an oder die PND id!</label>
          <label xml:lang="en">Please insert authors name.</label>
        </condition>
      </textfield>
    </cell>
    <cell col="2" row="2" anchor="NORTHEAST">
      <output var="nameOrPND/@editor.output" default="" />
    </cell>
  </panel>

  <panel id="mods.author">
    <cell anchor="NORTHWEST" var="mods:name[@type='personal']">
      <repeater min="1" max="20" pos="right">
        <panel>
          <hidden var="mods:role/mods:roleTerm/@authority" default="marcrelator" />
          <hidden var="mods:role/mods:roleTerm/@type" default="code" />
          <hidden var="mods:role/mods:roleTerm" default="aut" />
          <cell ref="mods.name"/>
        </panel>
      </repeater>
    </cell>
  </panel>

  <helpPopup id="mods.author.help" width="400px" height="250px">
    <label xml:lang="de">
      Die Person, die primär das Dokument erstellt hat. Weitere Autoren können über den "+" Button hinzugefügt werden.
      (Verlinkung auf PND)
    </label>
    <label xml:lang="en">
      Persons primarily responsible for making the resource. Further persons my be added with "+" button.
      (ref to pnd)
    </label>
  </helpPopup>
  <!-- pub_autor_dok_txt [text] and reviewer END -->

  <!-- person -->
  <text id="mods.person.label">
    <label xml:lang="de">Personen (*)</label>
    <label xml:lang="en">Persons (*)</label>
  </text>

  <panel id="mods.person">
    <cell anchor="NORTHWEST" var="mods:name[@type='personal']">
      <repeater min="1" max="20" pos="right">
        <panel>
          <cell col="1" row="1" anchor="NORTHEAST" var="mods:role/mods:roleTerm">
            <panel>
              <hidden var="@authority" default="marcrelator" />
              <hidden var="@type" default="code" />
              <cell>
                <list type="dropdown" default="aut" id="mods.person.list">
                  <include ref="pleaseSelect" />
                  <include uri="classification:editor:-1:children:marcrelator" cacheable="false" />
                  <condition id="mods.person.cond" required="true">
                    <label xml:lang="de">Bitte wählen Sie einen Wert aus!</label>
                    <label xml:lang="en">Please select one entry!</label>
                  </condition>
                  </list>
              </cell>
            </panel>
          </cell>
          <cell col="2" row="1" anchor="NORTHEAST" ref="mods.name" />
        </panel>
      </repeater>
    </cell>
  </panel>

  <helpPopup id="mods.person.help" width="400px" height="250px">
    <label xml:lang="de">
      Name und Rolle der Personen, in welcher sie bei der Erstellung des Dokumentes mitgewirkt haben.
      (Verlinkung auf
      PND).
    </label>
    <label xml:lang="en">
      Names and roles of persons cooperating .... (ref to pnd)
    </label>
  </helpPopup>
  <!-- person END -->

  <!-- Institutionszugehörigkeit (wo zugeordnet) -->
  <text id="mods.assignedInstitute.label">
    <label xml:lang="de">Institutionszugehörigkeit</label>
    <label xml:lang="en">Assignment</label>
  </text>

  <helpPopup id="mods.assignedInstitute.help" width="400px" height="250px">
    <label xml:lang="de">Institut/Fachgruppe/Organisationseinheit, zu welcher die Autoren bzw. die Beitragenden
      zum Zeitpunkt der Veröffentlichung des Dokumentes, des Erscheinungsjahres des Konferenzbandes
      oder Buches gehört haben.</label>
    <label xml:lang="en">Select an institution from the list.</label>
  </helpPopup>
  <!-- Institutionszugehörigkeit END -->

  <!-- Publisher [text] -->
  <text id="mods.publisher.label">
    <label xml:lang="de">Verleger (*)</label>
    <label xml:lang="en">Publisher (*)</label>
  </text>

  <panel id="mods.publisher">
    <cell anchor="NORTHWEST" var="mods:publisher">
      <panel>
        <cell anchor="NORTHWEST" >
          <textfield id="mods.publisher.text" width="60" maxlength="512" >
            <condition id="mods.publisher.cond" required="true">
              <label xml:lang="de">Tragen Sie hier den Verleger/Herausgeber ein (Druckerei, Medienanstalt ...)).</label>
              <label xml:lang="en">Enter publisher here.</label>
            </condition>
          </textfield>
        </cell>
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.publisher.help" width="400px" height="250px">
    <label xml:lang="de">
      Eine natürliche oder juristische Person oder Personengruppe, die als verantwortlich für die
      inhaltliche Gestaltung eines Dokuments angegeben wird, ohne selbst Verfasser des Dokuments oder einzelner
      Teile desselben sein zu müssen
    </label>
    <label xml:lang="en">
      Publisher
    </label>
  </helpPopup>
  <!-- Publisher END -->

  <!-- Ort der Konferenz [text] -->
  <text id="mods.conferencePlace.label">
    <label xml:lang="de">Ort der Konferenz (*)</label>
    <label xml:lang="en">City of Conference (*)</label>
  </text>

  <panel id="mods.conferencePlace">
    <cell anchor="NORTHWEST" var="mods:place/mods:placeTerm[@type='text']">
      <panel>
        <cell anchor="NORTHWEST">
          <textfield id="mods.conferencePlace.text" width="60" maxlength="512">
            <condition id="mods.conferencePlace.cond" required="true">
              <label xml:lang="de">Tragen Sie hier den Ort ein, an dem die Konferenz stattfand.</label>
              <label xml:lang="en">Enter place of conference here.</label>
            </condition>
          </textfield>
        </cell>
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.conferencePlace.help" width="400px" height="250px">
    <label xml:lang="de">
      Ort der Konferenz
    </label>
    <label xml:lang="en">
      Conference Place
    </label>
  </helpPopup>
  <!-- Ort der Konferenz END -->


  <!-- Land in dem die Konferenz stattfand [text] -->
  <text id="mods.conferenceLand.label">
    <label xml:lang="de">Land der Konferenz (*)</label>
    <label xml:lang="en">Conference Country (*)</label>
  </text>

  <panel id="mods.conferenceLand">
    <cell anchor="NORTHWEST" var="mods:place/mods:placeTerm[@authority='marccountry']">
      <hidden var="@type" default="code" /> <!-- ToDo: Klassifikation für Ländercodes -->
      <panel>
        <cell anchor="NORTHWEST">
          <textfield id="mods.conferenceLand.text" width="60" maxlength="512">
            <condition id="mods.conferenceLand.cond" required="true">
              <label xml:lang="de">Tragen Sie hier das Land ein, in dem die Konferenz stattfand.</label>
              <label xml:lang="en">Enter country of conference here.</label>
            </condition>
          </textfield>
        </cell>
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.conferenceLand.help" width="400px" height="250px">
    <label xml:lang="de">
      Das Land, in dem die Konferenz stattfand.
    </label>
    <label xml:lang="en">
      Country Place
    </label>
  </helpPopup>
  <!-- Land in dem die Konferenz stattfand END -->


  <!-- Institution der Einreichung -->
  <text id="mods.submissionAtInstitute.label">
    <label xml:lang="de">Hochschule/Institut der Einreichung</label>
    <label xml:lang="en">Submission at Intitute</label>
  </text>

  <helpPopup id="mods.submissionAtInstitute.help" width="400px" height="250px">
    <label xml:lang="de">Die Hochschule, an der das Dokument eingereicht wurde</label>
    <label xml:lang="en">Select an institution from the list.</label>
  </helpPopup>
  <!-- Institution END -->

  <!-- Datum der Veröffentlichung [text] -->
  <text id="mods.dateIssued.label">
    <label xml:lang="de">Veröffentlichungsdatum (*)</label>
    <label xml:lang="en">Date of Publishing (*)</label>
  </text>

  <panel id="mods.dateIssued">
    <cell anchor="NORTHWEST" var="mods:dateIssued">
      <panel>
        <cell anchor="NORTHWEST">
          <textfield id="mods.dateIssued.text" width="60" maxlength="512">
            <condition id="mods.dateIssued.cond" required="true">
              <label xml:lang="de">Tragen Sie hier das Datum der Veröffentlichung ein.</label>
              <label xml:lang="en">Enter the date of publication here.</label>
            </condition>
          </textfield>
        </cell>
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.dateIssued.help" width="400px" height="250px">
    <label xml:lang="de">
      Bei Konferenzbänden, Büchern geben Sie bitte das Erscheinungsjahr an, 
      bei Filmen das Datum der Aufnahme, bei Promotionen das Datum der ersten Online Veröffentlichung.
      Das Datum muss im ISO Format mit Bindestrich (YYYY-MM-DD) angegeben werden.<br /> 
      Beispiel: 2003-01-27 
    </label>
    <label xml:lang="en">
      Date of Publication:
      video recording date

    </label>
  </helpPopup>
  <!-- Datum der Veröffentlichung END -->

  <!-- Datum der Erstellung [text] -->
  <text id="mods.dateCreated.label">
    <label xml:lang="de">Erstellungsdatum</label>
    <label xml:lang="en">Date of Creation</label>
  </text>

  <panel id="mods.dateCreated">
    <cell anchor="NORTHWEST" var="mods:dateCreated">
      <panel>
        <cell col="1">
          <textfield width="60" maxlength="128" />
        </cell>
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.dateCreated.help" width="400px" height="250px">
    <label xml:lang="de">
      Datum der Erstellung
    </label>
    <label xml:lang="en">
      Date of Creation
    </label>
  </helpPopup>
  <!-- Datum der Erstellung END -->

  <!-- Zeit in der die Konferenz stattfand [text] -->
  <text id="mods.dateOccured.label">
    <label xml:lang="de">Zeitraum der Konferenz</label>
    <label xml:lang="en">Date of Conference</label>
  </text>

  <panel id="mods.dateOccured">
    <cell anchor="NORTHWEST">
      <panel>
        <cell col="1" row="1">
          <text>
            <label xml:lang="de">von</label>
            <label xml:lang="en">from</label>
          </text>
        </cell>
        <cell col="2" row="1" var="mods:dateOther[@point='start']">
          <textfield width="20" maxlength="40" />
        </cell>
        <cell col="3" row="1">
          <text>
            <label xml:lang="de">bis</label>
            <label xml:lang="en">until</label>
          </text>
        </cell>
        <cell col="4" row="1" var="mods:dateOther[@point='end']">
          <textfield width="20" maxlength="40" />
        </cell>
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.dateOccured.help" width="400px" height="250px">
    <label xml:lang="de">
      Zeitraum in der die Konferenz stattfand (von, bis). Das Datum muss im
      ISO Format mit Bindestrich (YYYY-MM-DD) angegeben werden.
      <br />
      Beispiel: 2003-01-27
    </label>
    <label xml:lang="en">
      Date of Conference (start, end). The date must be inserted in ISO format
      with dahes (YYYY-MM-DD).
      Sample: 2003-01-27
      <br />

    </label>
  </helpPopup>
  <!--Zeit in der die Konferenz stattfand END -->

  <!-- Abstract -->
  <text id="mods.abstract.label">
    <label xml:lang="de">Zusammenfassung</label>
    <label xml:lang="en">Abstract</label>
  </text>

  <panel id="mods.abstract">
    <cell col="2" row="50" anchor="NORTHWEST" var="mods:abstract">
      <repeater min="1" max="5" pos="right">
        <panel>
          <cell anchor="NORTHWEST" ref="TFarea4096" />
          <cell row="1" col="2" anchor="NORTHWEST" var="@lang">
            <list type="dropdown" default="de">
              <item value="de">
                <label xml:lang="de">deutsch</label>
                <label xml:lang="en">german</label>
              </item>
              <item value="en">
                <label xml:lang="de">englisch</label>
                <label xml:lang="en">english</label>
              </item>
              <item value="other">
                <label xml:lang="de">sontige</label>
                <label xml:lang="en">other</label>
              </item>
            </list>
          </cell>
        </panel>
      </repeater>
    </cell>
  </panel>

  <helpPopup id="mods.abstract.help" width="400px" height="250px">
    <label xml:lang="de">
      Eine kurze Zusammenfassung/Abstrakt des Dokumentinhaltes in verschiedenen Sprachen.
    </label>
    <label xml:lang="en">
      abstract (translated also)
    </label>
  </helpPopup>
  <!-- Abstract END -->

  <!-- Stichworte -->
  <text id="mods.subject.label">
    <label xml:lang="de">Stichworte</label>
    <label xml:lang="en">Subject</label>
  </text>

  <panel id="mods.subject">
    <cell anchor="NORTHWEST" var="mods:subject">
      <repeater min="1" max="500" pos="right" var="mods:topic">
        <panel>
          <cell anchor="NORTHWEST" ref="TFzeile512" />
        </panel>
      </repeater>
    </cell>
  </panel>

  <helpPopup id="mods.subject.help" width="400px" height="250px">
    <label xml:lang="de">
      Jedes Stichwort sollte in ein separates Eingabefeld geschrieben werden. 
      Bei mehreren Stichworten fügen Sie bitte mit dem "+" Button jeweils ein neues Feld hinzu.
    </label>
    <label xml:lang="en">
      Subject
    </label>
  </helpPopup>
  <!-- Stichworte END -->
  
  <!-- dreier repeater -->
  <panel id="mods.noteLocationCorp">
    <cell var="noteLocationCorp">
      <repeater>
        <panel>
          <cell row="1" col="1" anchor="NORTHEAST" ref="mods.assignedInstitute.label" />
          <cell row="1" col="2" anchor="NORTHWEST" ref="mods.assignedInstitute" />
          <cell row="1" col="3" anchor="NORTHWEST" ref="mods.assignedInstitute.help" />
          <cell row="20" col="1" anchor="NORTHEAST" ref="mods.note.label" />
          <cell row="20" col="2" anchor="NORTHWEST" ref="mods.note" />
          <cell row="20" col="3" anchor="NORTHWEST" ref="mods.note.help" />
          <cell row="30" col="1" anchor="NORTHEAST" ref="mods.physicalLocation.label" />
          <cell row="30" col="2" anchor="NORTHWEST" ref="mods.physicalLocation" var="mods:location" />
          <cell row="30" col="3" anchor="NORTHWEST" ref="mods.physicalLocation.help" />
        </panel>
      </repeater>
    </cell>
  </panel>

  <!-- Kommentar -->
  <text id="mods.note.label">
    <label xml:lang="de">Kommentar</label>
    <label xml:lang="en">Comment</label>
  </text>

  <panel id="mods.note">
    <cell anchor="NORTHWEST" var="mods:note" ref="TFarea4096" />
  </panel>

  <helpPopup id="mods.note.help" width="400px" height="250px">
    <label xml:lang="de">
      Hier können beliebige Kommentare zum Dokument notiert werden
    </label>
    <label xml:lang="en">
      Comment
    </label>
  </helpPopup>
  <!-- Kommentar END -->

  <!-- Standort -->
  <text id="mods.physicalLocation.label">
    <label xml:lang="de">Standort</label>
    <label xml:lang="en">Location</label>
  </text>
  <panel id="mods.physicalLocation">
    <cell anchor="NORTHWEST" var="mods:physicalLocation">
      <panel>
        <cell anchor="NORTHWEST" ref="TFzeile512" />
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.physicalLocation.help" width="400px" height="250px">
    <label xml:lang="de">
      Standort der physischen Ressource (Buch, Konferenzband, Film)
    </label>
    <label xml:lang="en">
      Location of physical resource (book, conference proceeding, film)
    </label>
  </helpPopup>
  <!-- Standort END -->

  <!-- Quelle -->
  <text id="mods.relatedItem.label">
    <label xml:lang="de">2. Quelle</label>
    <label xml:lang="en">2nd source</label>
  </text>

  <panel id="mods.relatedItem">
    <cell anchor="NORTHWEST" var="mods:relatedItem[@type='isReferencedBy']/mods:titleInfo/mods:title">
      <panel>
        <cell anchor="NORTHWEST" ref="TFzeile512" />
      </panel>
    </cell>
  </panel>

  <helpPopup id="mods.relatedItem.help" width="400px" height="250px">
    <label xml:lang="de">
      Gedruckte oder elektronische Quelle einer sonstigen Veröffentlichung des vorliegenden
      Dokumentes.
      <br />
      Für ein retrodigitalisiertes Dokument wäre das beispielsweise die Printversion.
      Bei Postern oder Abstracts kann hier die Konferenz oder der Konferenzbeitrag, in deren Kontext
      dieses Dokument steht, angegeben werden.
    </label>
    <label xml:lang="en">
      Source
    </label>
  </helpPopup>
  <!-- Quelle END -->

  <!-- Identifier -->
  <text id="mods.identifier.label">
    <label xml:lang="de">Identifier</label>
    <label xml:lang="en">Identifier</label>
  </text>

  <panel id="mods.identifier">
    <cell anchor="NORTHWEST" var="mods:identifier">
      <repeater min="1" max="5" pos="right">
