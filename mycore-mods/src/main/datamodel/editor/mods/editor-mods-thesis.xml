<?xml version="1.0" encoding="UTF-8"?>

<imports>
  <include uri="webapp:editor/editor-mods-common.xml" />

  <panel id="edit-mods-thesis">
    <hidden var="@ID" />
    <hidden var="@label" />
    <hidden var="@version" />
    <hidden var="service" descendants="true" />
    <hidden var="metadata/@xml:lang" default="de" />

    <hidden var="metadata/def.modsContainer/modsContainer/mods:mods/mods:typeOfResource" default="text" />
    <hidden var="metadata/def.modsContainer/modsContainer/mods:mods/mods:genre[@type='intern']/@mcr:categId" default="bmelv_genres:thesis" />
    <hidden var="metadata/def.modsContainer/modsContainer/mods:mods/@ID" />
    <hidden var="metadata/def.modsContainer/@class" default="MCRMetaXML" />
    <hidden var="metadata/def.modsContainer/@notinherit" default="true" />
    <hidden var="metadata/def.modsContainer/@heritable" default="false" />

    <!-- Basisangaben zum Dokument -->
    <cell row="1" colspan="3" anchor="NORTHWEST" class="editorSubHeadlines">
      <text id="thesis.titles" i18n="metaData.mods.dictionary.thesis" />
    </cell>
    
    <!-- pub_title_orig [text] -->
    <cell row="10" col="1" class="mandatory" anchor="NORTHEAST" ref="mods.title.label" style="padding-top:5px;" />
    <cell row="10" col="2" anchor="NORTHWEST" ref="mods.title" var="metadata/def.modsContainer/modsContainer/mods:mods/mods:titleInfo" style="padding-top:5px;" />
    <cell row="10" col="3" anchor="NORTHWEST" ref="mods.title.help" style="padding-top:5px;" />
    <!-- pub_title_orig [text] END -->

    <!-- pub_title_html [text] -->
    <cell row="15" col="1" anchor="NORTHEAST" ref="mods.htmltitle.label" />
    <cell row="15" col="2" anchor="NORTHWEST" ref="mods.htmltitle" var="metadata/def.modsContainer/modsContainer/mods:mods/mods:titleInfo[@lang='x-html']" />
    <cell row="15" col="3" anchor="NORTHWEST" ref="mods.htmltitle.help" />
    <!-- pub_title_html [text] END -->

    <!-- pub_title_engl[text] -->
    <cell row="20" col="1" anchor="NORTHEAST" ref="mods.title.translated.label" />
    <cell row="20" col="2" anchor="NORTHWEST" ref="mods.title.translated" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="20" col="3" anchor="NORTHWEST" ref="mods.title.translated.help" />
    <!-- pub_title_engl[text] END -->

    <!-- pub_subtitle [text] -->
    <cell row="25" col="1" anchor="NORTHEAST" ref="mods.subtitle.label" />
    <cell row="25" col="2" anchor="NORTHWEST" ref="mods.subtitle" var="metadata/def.modsContainer/modsContainer/mods:mods/mods:titleInfo" />
    <cell row="25" col="3" anchor="NORTHWEST" ref="mods.subtitle.help" />
    <!-- pub_subtitle [text] END -->

    <!-- pub_autor_dok_txt [text] and reviewer -->
    <cell row="30" col="1" class="mandatory" anchor="NORTHEAST" ref="mods.person.label" />
    <cell row="30" col="2" anchor="NORTHWEST" ref="mods.person" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="30" col="3" anchor="NORTHWEST" ref="mods.person.help" />
    <!-- pub_autor_dok_txt [text] and reviewer END -->
    
    <!-- Datum der Veröffentlichung [text] -->
    <cell row="35" col="1" class="mandatory" anchor="NORTHEAST" ref="mods.dateIssued.label" />
    <cell row="35" col="2" anchor="NORTHWEST" ref="mods.dateIssued" var="metadata/def.modsContainer/modsContainer/mods:mods/mods:originInfo" />
    <cell row="35" col="3" anchor="NORTHWEST" ref="mods.dateIssued.help" />
    <!-- Datum der Veröffentlichung END -->
    
   <!-- Institution der Einreichung -->
    <cell row="40" col="1" anchor="NORTHEAST" ref="mods.submissionAtInstitute.label" style="padding-bottom:10px;" />
    <cell row="40" col="2" anchor="NORTHWEST" ref="mods.submissionAtInstitute" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="40" col="3" anchor="NORTHWEST" ref="mods.submissionAtInstitute.help" />
    <!-- Institution END --> 
    
  <!--  Erweiterte Angaben zum Dokument -->
    <cell row="50" colspan="3" anchor="NORTHWEST" class="editorSubHeadlines">
      <text id="thesis.more" i18n="metaData.mods.dictionary.categorybox" />
    </cell>
    
    <!-- genre classification -->
    <cell row="60" col="1" class="mandatory" anchor="NORTHEAST" ref="mods.genre.kindOfThesis.label" style="padding-top:5px;" />
    <cell row="60" col="2" anchor="NORTHWEST" ref="mods.genre.kindOfThesis" var="metadata/def.modsContainer/modsContainer/mods:mods" style="padding-top:5px;" />
    <cell row="60" col="3" anchor="NORTHWEST" ref="mods.genre.kindOfThesis.help" style="padding-top:5px;" />
    <!-- genre classification END -->
    <!-- LANGUAGE -->
    <cell row="65" col="1" class="mandatory" anchor="NORTHEAST" ref="mods.languageTerm.label" />
    <cell row="65" col="2" anchor="NORTHWEST" ref="mods.languageTerm" var="metadata/def.modsContainer/modsContainer/mods:mods/mods:language" />
    <cell row="65" col="3" anchor="NORTHWEST" ref="mods.languageTerm.help" />
    <!-- LANGUAGE END -->

    <!-- Abstract -->
    <cell row="70" col="1" anchor="NORTHEAST" ref="mods.abstract.label" />
    <cell row="70" col="2" anchor="NORTHWEST" ref="mods.abstract" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="70" col="3" anchor="NORTHWEST" ref="mods.abstract.help" />
    <!-- Abstract END -->

    <!-- Extent|Part -->
    <cell row="75" col="1" anchor="NORTHEAST" ref="mods.book.part.extent.label" />
    <cell row="75" col="2" anchor="NORTHWEST" ref="mods.book.part.extent" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="75" col="3" anchor="NORTHWEST" ref="mods.book.part.extent.help" />
    <!-- Extent|Part END -->

    <!-- pub_klassifik -->
    <cell row="80" col="1" class="mandatory" anchor="NORTHEAST" ref="mods.ddc.label" />
    <cell row="80" col="2" anchor="NORTHWEST" ref="mods.ddc" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="80" col="3" anchor="NORTHWEST" ref="mods.ddc.help" />
    <!-- pub_klassifik END -->
    
    <!-- Stichworte -->
    <cell row="85" col="1" anchor="NORTHEAST" ref="mods.subject.label" />
    <cell row="85" col="2" anchor="NORTHWEST" ref="mods.subject" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="85" col="3" anchor="NORTHWEST" ref="mods.subject.help" />
    <!-- Stichworte END -->
    
    <!-- Rechte -->
    <cell row="90" col="1" anchor="NORTHEAST" ref="mods.rights.label" />
    <cell row="90" col="2" anchor="NORTHWEST" ref="mods.rights" var="metadata/def.modsContainer/modsContainer/mods:mods" style="padding-bottom:10px;" />
    <cell row="90" col="3" anchor="NORTHWEST" ref="mods.rights.help" />
    <!-- Rechte END -->
    
    <!-- Identifier Panel -->
    <cell row="100" colspan="3" anchor="NORTHWEST" class="editorSubHeadlines">
      <text id="thesis.idetifier" i18n="metaData.mods.dictionary.identifier.box" />
    </cell>

    <!-- Identifier -->
    <cell row="140" col="1" anchor="NORTHEAST" ref="mods.identifier.label" style="padding-top:5px;" />
    <cell row="140" col="2" anchor="NORTHWEST" ref="mods.identifier" var="metadata/def.modsContainer/modsContainer/mods:mods" style="padding-top:5px;" />
    <cell row="140" col="3" anchor="NORTHWEST" ref="mods.identifier.help" style="padding-top:5px;" />
    <!-- Identifier END -->

    <!-- URL -->
    <cell row="145" col="1" anchor="NORTHEAST" ref="mods.location.url.label" />
    <cell row="145" col="2" anchor="NORTHWEST" ref="mods.location.url" var="metadata/def.modsContainer/modsContainer/mods:mods/mods:location" />
    <cell row="145" col="3" anchor="NORTHWEST" ref="mods.location.url.help" />
    <cell row="146" col="1" anchor="NORTHEAST" ref="mods.location.url.displayLabel.label" style="padding-bottom:10px;" />
    <cell row="146" col="2" anchor="NORTHWEST" ref="mods.location.url.displayLabel" var="metadata/def.modsContainer/modsContainer/mods:mods/mods:location" />
    <cell row="146" col="3" anchor="NORTHWEST" ref="mods.location.url.displayLabel.help" />
    <!-- URL END -->

  <!-- Interne Daten -->
    <cell row="148" colspan="3" anchor="NORTHWEST" class="editorSubHeadlines">
      <text id="thesis.internals" i18n="metaData.mods.dictionary.institution.box" />
    </cell>
    <!-- arev_klassifik -->
    <cell row="150" col="1" anchor="NORTHEAST" ref="mods.arev.label" style="padding-top:5px;" />
    <cell row="150" col="2" anchor="NORTHWEST" ref="mods.arev" var="metadata/def.modsContainer/modsContainer/mods:mods" style="padding-top:5px;" />
    <cell row="150" col="3" anchor="NORTHWEST" ref="mods.arev.help" style="padding-top:5px;" />
    <!-- arev_klassifik END -->
  
    <!-- arev_jahr -->
    <cell row="155" col="1" anchor="NORTHEAST" ref="mods.arev.year.label" />
    <cell row="155" col="2" anchor="NORTHWEST" ref="mods.arev.year" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="155" col="3" anchor="NORTHWEST" ref="mods.arev.year.help" />
    <!-- arev_jahr END -->


    <!-- Institution-Tripple -->
    <cell row="160" col="1" colspan="3" anchor="NORTHWEST" ref="mods.noteLocationCorp" var="metadata/def.modsContainer/modsContainer/mods:mods" />


    <!-- save cancel buttons -->
    <cell col="1" colspan="3" row="250" anchor="NORTHEAST">
      <submitButton width="100px" i18n="common.button.save" />
      <cancelButton width="100px" i18n="common.button.cancel" />
    </cell>
  </panel>

</imports>
