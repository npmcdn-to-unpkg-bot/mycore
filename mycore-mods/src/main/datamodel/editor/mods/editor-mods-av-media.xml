    <cell row="20" col="1" anchor="NORTHEAST" ref="mods.person.label" />
    <cell row="20" col="2" anchor="NORTHWEST" ref="mods.person" var="metadata/def.modsContainer/modsContainer/mods:mods" />
    <cell row="20" col="3" anchor="NORTHWEST" ref="mods.person.help" />
